{
    "name": "titlewdiv",
    "type": "template",
    "content": [
        {
            "name": "div",
            "attr": {
                "style": {
                    "text-align": "$_twd_template_align$"
                }
            },
            "content": [
                {
                    "name": "$_twd_template_titletype$",
                    "content": ["$_twd_template_title$"]
                },
                {
                    "name": "cdiv",
                    "vars": {
                        "_cdiv_template_content": ["$_twd_template_paragraph$"],
                        "_cdiv_template_maxwidth": "$_twd_template_maxwidth$"
                    }
                }
            ]
        }
    ],
    "vars": {
        "_twd_template_title": "Title",
        "_twd_template_paragraph": "Some content in Paragraph",
        "_twd_template_maxwidth": "80%",
        "_twd_template_align": "center",
        "_twd_template_titletype": "h3"
    }
}