{
    "name": "titlewdiv",
    "type": "template",
    "content": [
        {
            "name": "div",
            "attr": {
                "style": {
                    "text-align": "$twd_template_align$"
                }
            },
            "content": [
                {
                    "name": "h3",
                    "content": ["$twd_template_title$"]
                },
                {
                    "name": "cdiv",
                    "vars": {
                        "cdiv_template_content": ["$twd_template_paragraph$"],
                        "cdiv_template_maxwidth": "$twd_template_maxwidth$"
                    }
                }
            ]
        }
    ],
    "vars": {
        "twd_template_maxwidth": "800px",
        "twd_template_align": "center"
    }
}