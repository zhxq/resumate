{
    "type": "template",
    "name": "infobox",
    "css": ["./css/infobox.css"],
    "js": ["./js/infobox.js"],
    "default": {
        "attr": {
            
        }
    },
    "content": [
        {
            "name": "div",
            "attr": {
                "style": {
                    "width": "$ib_template_width$",
                    "height": "$ib_template_height$"
                },
                "attrtest": "$ib_template_attrtest$",
                "class": ["ib_template_infobox"]
            },
            "content": [
                {
                    "name": "p",
                    "content": [
                        {
                            "name": "pic",
                            "attr": {
                                "style": {
                                    "width": "$ib_template_picwidth$",
                                    "height": "$ib_template_picheight$",
                                    "border-radius": "50%"
                                },
                                "attrtest": "$ib_template_attrtest$",
                                "class": ["infobox"],
                                "src": "$ib_template_src$"
                            }
                        }
                    ]
                },
                {
                    "name": "h1",
                    "content": ["$ib_template_title$"]
                },
                {
                    "name": "h4",
                    "content": ["$ib_template_line1$"]
                },
                {
                    "name": "h4",
                    "content": ["$ib_template_line2$"]
                },
                {
                    "name": "h4",
                    "content": ["$ib_template_line3$"]
                },
                {
                    "name": "hr",
                    "content": [""]
                }
            ]
        }
    ],
    "vars": {
        "ib_template_title": "PLACEHOLDER",
        "ib_template_line1": "This is the line for line 1.",
        "ib_template_line2": "This is the line for line 2.",
        "ib_template_line3": "This is the line for line 3.",
        "ib_template_src": "/img/test.png",
        "ib_template_attrtest": "infobox_def_default"
    }
}