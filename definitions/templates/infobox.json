{
    "type": "template",
    "name": "infobox",
    "css": ["./css/infobox.css"],
    "js": ["./js/infobox.js"],
    "content": [
        {
            "name": "div",
            "attr": {
                "style": {
                    "width": "$_ib_template_width$",
                    "height": "$_ib_template_height$"
                }
            },
            "content": [
                {
                    "name": "p",
                    "content": [
                        {
                            "name": "pic",
                            "attr": {
                                "style": {
                                    "width": "$_ib_template_picwidth$",
                                    "height": "$_ib_template_picheight$",
                                    "border-radius": "50%"
                                },
                                "src": "$_ib_template_src$"
                            }
                        }
                    ]
                },
                {
                    "name": "h1",
                    "content": ["$_ib_template_title$"]
                },
                {
                    "name": "h4",
                    "content": ["$_ib_template_line1$"]
                },
                {
                    "name": "h4",
                    "content": ["$_ib_template_line2$"]
                },
                {
                    "name": "h4",
                    "content": ["$_ib_template_line3$"]
                },
                {
                    "name": "hr",
                    "content": [""]
                }
            ]
        }
    ],
    "vars": {
        "_ib_template_title": "PLACEHOLDER",
        "_ib_template_picwidth": "225px",
        "_ib_template_picheight": "225px",
        "_ib_template_line1": "This is the line for line 1.",
        "_ib_template_line2": "This is the line for line 2.",
        "_ib_template_line3": "This is the line for line 3.",
        "_ib_template_src": "/img/test.png"
    }
}