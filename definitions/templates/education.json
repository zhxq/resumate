{
    "name": "education",
    "type": "template",
    "content": [
        {
            "name": "h3",
            "content": ["$_edu_template_title$"]
        },
        {
            "name": "h5",
            "content": ["$_edu_template_phd$"]
        },
        {
            "name": "h5",
            "content": ["$_edu_template_master$"]
        },
        {
            "name": "h5",
            "attr": {
                "style": {
                    "margin-bottom": "30px"
                }
            },
            "content": ["$_edu_template_bachelor$"]
        },
        {
            "name": "cpara",
            "vars": {
                "_cpara_template_content": ["$_edu_template_comment$"],
                "_cpara_template_maxwidth": "800px"
            }
        }
    ],
    "vars": {
        "_edu_template_title": "Education",
        "_edu_template_phd": "Insert your PhD experience, if any",
        "_edu_template_master": "Insert your Master experience, if any",
        "_edu_template_bachelor": "Insert your Bachelor experience, if any",
        "_edu_template_comment": "Explain and elaborate something more about your education experience here :)"
    }
}