{
    "name": "outofboxtemplateslist",
    "type": "block",
    "content": [
        {
            "name": "titlewdiv",
            "vars": {
                "_twd_template_title": "$outofboxtemplateslisttitle$",
                "_twd_template_paragraph": [
                    "<span>", 
                    {
                        "name": "link", 
                        "vars": {
                            "_lnk_template_link": "https://github.com/zhxq/resumate/blob/master/definitions/blocks/outofboxtemplateslist.json",
                            "_lnk_template_description": "$codeforthisblock$"
                        }
                    },
                    "</span><br><br>",
                    {
                        "name": "ul",
                        "content": [
                            "$cdivintro$",
                            "$cparaintro$",
                            "$educationintro$",
                            "$infoboxintro$",
                            "$jsoncodeintro$",
                            "$linkintro$",
                            "$picdescintro$",
                            "$publicationsintro$",
                            "$reverseemailintro$",
                            "$templateintrointro$",
                            "$titlewdivintro$",
                            "$titlewparaintro$",
                            "$workexpintro$"
                        ]
                    }
                ],
                "_twd_template_align": "left",
                "_twd_template_maxwidth": "100%"
            }
        }
    ],
    "vars": {
        "cdivintro": [
            {
                "name": "templateintro",
                "vars": {
                    "_ti_template_title": "cdiv",
                    "_ti_template_intro": "$cdivintrodescription$",
                    "_ti_template_macros": [
                        ["<code>_cdiv_template_content</code>: ", "$cdivintrocontent$"],
                        ["<code>_cdiv_template_maxwidth</code>: ", "$cdivintromaxwidth$"],
                        ["<code>_cdiv_template_width</code>: ", "$cdivintrowidth$"],
                        ["<code>_cdiv_template_height</code>: ", "$cdivintroheight$"],
                        ["<code>_cdiv_template_padding</code>: ", "$cdivintropadding$"]
                    ]
                }
            }
        ],
        "cparaintro": [
            {
                "name": "templateintro",
                "vars": {
                    "_ti_template_title": "cpara",
                    "_ti_template_intro": "$cparaintrodescription$",
                    "_ti_template_macros": [
                        ["<code>_cpara_template_content</code>: ", "$cparaintrocontent$"],
                        ["<code>_cpara_template_maxwidth</code>: ", "$cparaintromaxwidth$"],
                        ["<code>_cpara_template_width</code>: ", "$cparaintrowidth$"],
                        ["<code>_cpara_template_height</code>: ", "$cparaintroheight$"],
                        ["<code>_cpara_template_padding</code>: ", "$cparaintropadding$"]
                    ]
                }
            }
        ],
        "educationintro": [
            {
                "name": "templateintro",
                "vars": {
                    "_ti_template_title": "education",
                    "_ti_template_intro": "$educationintrodescription$",
                    "_ti_template_macros": [
                        ["<code>_edu_template_title</code>: ", "$educationintrotitle$"],
                        ["<code>_edu_template_phd</code>: ", "$educationintrophd$"],
                        ["<code>_edu_template_master</code>: ", "$educationintromaster$"],
                        ["<code>_edu_template_bachelor</code>: ", "$educationintrobachelor$"],
                        ["<code>_edu_template_comment</code>: ", "$educationintrocomment$"]
                    ]
                }
            }
        ],
        "infoboxintro": [
            {
                "name": "templateintro",
                "vars": {
                    "_ti_template_title": "infobox",
                    "_ti_template_intro": "$infoboxintrodescription$",
                    "_ti_template_macros": [
                        ["<code>_ib_template_src</code>: ", "$infoboxintrosrc$"],
                        ["<code>_ib_template_picwidth</code>: ", "$infoboxintropicwidth$"],
                        ["<code>_ib_template_picheight</code>: ", "$infoboxintropicheight$"],
                        ["<code>_ib_template_title</code>: ", "$infoboxintropictitle$"],
                        ["<code>_ib_template_line1</code>: ", "$infoboxintropicline1$"],
                        ["<code>_ib_template_line2</code>: ", "$infoboxintropicline2$"],
                        ["<code>_ib_template_line3</code>: ", "$infoboxintropicline3$"]
                    ]
                }
            }
        ],
        "jsoncodeintro": [
            {
                "name": "templateintro",
                "vars": {
                    "_ti_template_title": "jsoncode",
                    "_ti_template_intro": "$jsoncodeintrodescription$",
                    "_ti_template_macros": [
                        ["<code>_jc_template_json_id</code>: ", "$jsoncodeintroid$"],
                        ["<code>_jc_template_json</code>: ", "$jsoncodeintropicjson$"]
                    ]
                }
            }
        ],
        "linkintro": [
            {
                "name": "templateintro",
                "vars": {
                    "_ti_template_title": "link",
                    "_ti_template_intro": "$linkintroduction$",
                    "_ti_template_macros": [
                        ["<code>_lnk_template_link</code>: ", "$linkintrolink$"],
                        ["<code>_lnk_template_description</code>: ", "$linkintrodescription$"]
                    ]
