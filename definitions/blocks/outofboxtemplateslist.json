                    "_ti_template_intro": "$reverseemailintroduction$",
                    "_ti_template_macros": [
                        ["<code>_re_template_email</code>: ", "$reverseemailintroreversedemail$"]
                    ]
                }
            }
        ],
        "templateintrointro": [
            {
                "name": "templateintro",
                "vars": {
                    "_ti_template_title": "templateintro",
                    "_ti_template_intro": "$templateintroduction$",
                    "_ti_template_macros": [
                        ["<code>_ti_template_title</code>: ", "$templateintroductiontitle$"],
                        ["<code>_ti_template_intro</code>: ", "$templateintroductionintro$"],
                        ["<code>_ti_template_macros</code>: ", "$templateintroductionmacros$"]
                    ]
                }
            }
        ],
        "titlewdivintro": [
            {
