                        ["<code>_pdesc_template_width</code>: ", "$pdescintrowidth$"],
                        ["<code>_pdesc_template_height</code>: ", "$pdescintroheight$"],
                        ["<code>_pdesc_template_picwidth</code>: ", "$pdescintropicwidth$"],
                        ["<code>_pdesc_template_picheight</code>: ", "$pdescintropicheight$"]
                    ]
                }
            }
        ],
        "publicationsintro": [
            {
                "name": "templateintro",
                "vars": {
                    "_ti_template_title": "publications",
                    "_ti_template_intro": "$publicationsintroduction$",
