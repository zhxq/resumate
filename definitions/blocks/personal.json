{
    "name": "personal",
    "type": "block",
    "default": {
        "attr": {
            
        }
    },
    "content": [
        {
            "name": "infobox",
            "attr": {
                "style": {
                    
                }
            },
            "vars":{
                "ib_template_title": "$title$",
                "ib_template_line1": "$line1$",
                "ib_template_line2": "$line2$",
                "ib_template_line3": "$line3$",
                "ib_template_src": "/imgs/logo.svg",
                "ib_template_picwidth": "225px",
                "ib_template_picheight": "225px"
            }
        },
        {
            "name": "titlewpara",
            "vars": {
                "twp_template_title": "$whatisresumate$",
                "twp_template_paragraph": "$resumateintro$"
            }
        },
        {
            "name": "education",
            "vars": {
                "edu_template_title": "$educationexp$",
                "edu_template_phd": [
                    "$educationphdstart$", 
                    {
                        "name": "a",
                        "attr": {
                            "href": "https://resumate.io/"
                        },
                        "content": ["$educationexampleprof$"]
                    }, 
                    "$educationphdend$"
                ],
                "edu_template_master": "$educationms$",
                "edu_template_bachelor": "$educationbachelor$",
                "edu_template_comment": "$educationdescription$"
            }
        },
        {
            "name": "titlewdiv",
            "vars": {
                "twp_template_title": "$codeforabove$",
                "twp_template_paragraph": [
                    {
                        "name": "jsoncode",
                        "vars": {
                            "jc_template_json_id": "codeforabovecodejsvar",
                            "jc_template_json": "$codeforabovecode$"
                        }
                    }
                ]
            }
        },
        {
            "name": "titlewdiv",
            "vars": {
                "twp_template_title": "$samplecodeexplain$",
                "twp_template_paragraph": "$samplecodeexplainmain$"
            }
        }
    ],
    "vars":{
        "title": "Testing!",
        "line3": "This is a new placeholder for line3"
    }
}