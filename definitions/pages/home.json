{
    "name": "home",
    "type": "page",
    "content": [
        {
            "name": "infobox",
            "vars":{
                "_ib_template_title": "$sitemenutitle$",
                "_ib_template_line1": "$introline1$",
                "_ib_template_line2": "$introline2$",
                "_ib_template_line3": [
                    {
                        "name": "link",
                        "vars": {
                            "link": "https://resumate.io/",
                            "description": "https://resumate.io/"
                        }
                    },
                    " - "
                    ,
                    {
                        "name": "link",
                        "vars": {
                            "link": "https://github.com/zhxq/resumate",
                            "description": "GitHub"
                        }
                    }
                ],
                "_ib_template_src": "/imgs/logo.svg",
                "_ib_template_picwidth": "225px",
                "_ib_template_picheight": "225px"
            }
        },
        {
            "name": "titlewpara",
            "vars": {
                "_twp_template_title": "$whatisresumate$",
                "_twp_template_paragraph": "$resumateintro$"
            }
        },
        {
            "name": "education",
            "vars": {
                "_edu_template_title": "$educationexp$",
                "_edu_template_phd": [
                    "$educationphdstart$", 
                    {
                        "name": "a",
                        "attr": {
                            "href": "https://resumate.io/"
                        },
                        "content": ["$educationexampleprof$"]
                    }, 
                    "$educationphdend$"
                ],
                "_edu_template_master": "$educationms$",
                "_edu_template_bachelor": "$educationbachelor$",
                "_edu_template_comment": "$educationdescription$"
            }
        },
        {
            "name": "titlewdiv",
            "vars": {
                "_twd_template_title": "$codeforabove$",
                "_twd_template_paragraph": [
                    {
                        "name": "jsoncode",
                        "vars": {
                            "_jc_template_json_id": "codeforabovecodejsvar",
                            "_jc_template_json": "$codeforabovecode$"
                        }
                    }
                ]
            }
        },
        {
            "name": "titlewdiv",
            "vars": {
                "_twd_template_title": "$samplecodeexplain$",
                "_twd_template_paragraph": "$samplecodeexplainmain$"
            }
        }
    ],
    "vars": {
        "codeforabovecodejsvar": "codeforabovecodeid",
        "codeforabovecode": "{\"name\":\"education\",\"vars\":{\"_edu_template_title\":\"$educationexp$\",\"_edu_template_phd\":[\"$educationphdstart$\",{\"name\":\"link\",\"vars\":{\"link\":\"https://resumate.io/\",\"description\":\"$educationexampleprof$\"}},\"$educationphdend$\"],\"_edu_template_master\":\"$educationms$\",\"_edu_template_bachelor\":\"$educationbachelor$\",\"_edu_template_comment\":\"$educationdescription$\"}}"
    }
}